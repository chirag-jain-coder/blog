<%- include("partials/header"); -%>

<script src="lodash.js"></script>
<h1 id="home">Home</h1>
<br>
<p><%= homecontent %></p>
<% posts.forEach(function(post) { %>
   <h1><%= post.title %></h1>
    <!-- /* ejs tag used *  -->
  <p><%= post.content.substring(0,100 ) +"..." %>
    <a href="/posts/<%=post.title %>" class="show_hide" data-content="toggle-text">Read More</a>
  </p>

<%}); %>

<%- include("partials/footer"); -%>
